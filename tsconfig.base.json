{
    "compileOnSave": false,
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "declaration": true,
        "declarationMap": true,
        "noEmit": false,
        "noEmitHelpers": true,
      "composite": true,
      "pretty": true,
      "module": "Node16",
      "moduleResolution": "Node16",
      "target": "ES2022",
      "downlevelIteration": true,
      "sourceMap": true,
      "noEmitOnError": false,
      "importHelpers": true,
      "stripInternal": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictPropertyInitialization": false,
      "strictFunctionTypes": false,
      "useUnknownInCatchVariables": false,
      "jsx": "react-jsx",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext.asynciterable",
        "ES2021",
      ],
      "types": ["node", "jest"],
      "baseUrl": ".",
      "outDir": "./dist/ts",
      "rootDir": "src"
    }
  }